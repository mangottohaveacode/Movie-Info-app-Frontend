{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:react/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "requireConfigFile": false,
    "ecmaVersion": 12
  },
  "rules": {
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-console": 1,
    "no-restricted-syntax": 0
  }
}
